<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/coffee_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/coffee_icon"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/Theme.Cafes"
        tools:targetApi="31">

        <activity
            android:name=".screens.DetailUser"
            android:exported="false"
            android:label="@string/title_activity_detail_user"
            android:theme="@style/Theme.Cafes" />
        <activity
            android:name=".screens.CartonDetail"
            android:exported="false"
            android:label="@string/title_activity_carton_detail"
            android:theme="@style/Theme.Cafes" />
        <activity
            android:name=".screens.Users"
            android:exported="false"
            android:label="@string/title_activity_users"
            android:theme="@style/Theme.Cafes" />
        <activity
            android:name=".screens.Registro"
            android:exported="false"
            android:label="@string/title_activity_registro"
            android:theme="@style/Theme.Cafes">
        </activity>
        <activity
            android:name=".screens.DetailCarton"
            android:exported="false"
            android:label="@string/title_activity_detail_carton"
            android:theme="@style/Theme.Cafes">
        </activity>
        <activity
            android:name=".screens.Login"
            android:exported="true"
            android:label="@string/title_activity_login"
            android:theme="@style/Theme.Cafes">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".screens.MainActivity"
            android:exported="false"
            android:label="@string/title_activity_main"
            android:theme="@style/Theme.Cafes">
        </activity>
    </application>

</manifest>